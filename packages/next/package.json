{
  "name": "inline-i18n-multi-next",
  "version": "0.5.0",
  "description": "Next.js integration for inline-i18n-multi",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.mjs",
      "require": "./dist/server.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.mjs",
      "require": "./dist/client.js"
    },
    "./middleware": {
      "types": "./dist/middleware.d.ts",
      "import": "./dist/middleware.mjs",
      "require": "./dist/middleware.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "i18n",
    "next.js",
    "nextjs",
    "internationalization",
    "translation",
    "inline"
  ],
  "author": "exiivy98",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/exiivy98/inline-i18n-multi.git",
    "directory": "packages/next"
  },
  "peerDependencies": {
    "next": "^13.0.0 || ^14.0.0 || ^15.0.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "inline-i18n-multi": "workspace:*",
    "inline-i18n-multi-react": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^19.0.2",
    "next": "^15.1.3",
    "react": "^19.0.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
